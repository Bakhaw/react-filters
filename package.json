  },
  "homepage": "https://github.com/ritz078/react-component-boilerplate",
  "devDependencies": {
    "@kadira/storybook": "^1.10.1",
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "commitizen": "^2.7.6",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "eventsource-polyfill": "*",
    "expect": "^1.16.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
